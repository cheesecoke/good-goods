{
  "version": 2,
  "functions": {
    "api/items": {
      "maxDuration": 55,
      "memory": 1024
    },
    "api/[company]": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "env": {
    "MONGODB_URI": "@mongodb_uri"
  },
  "rewrites": [
    {
      "source": "/clothing.json",
      "destination": "/api/items"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
